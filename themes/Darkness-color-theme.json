	"tokenColors": [
		{
			"name": "comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#5F6672",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "functions",
			"scope": [
				"entity",
				"entity.name.function",
				"punctuation.definition.entity",
				"support.function"
			],
			"settings": {
				"foreground": "#D19A66"
			}
		},
		{
			"name": "keywords",
			"scope": [
				"keyword",
				"punctuation.definition.keyword"
			],
			"settings": {
